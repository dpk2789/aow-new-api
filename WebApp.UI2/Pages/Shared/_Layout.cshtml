<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Accounting On Web | Online Accounting | Billing | Invoice | Bill Print</title>
    <meta name="description"
          content="India's biggest online store for Packaging Boxes, Fashion (Clothes/Shoes),
    Personal Care and more! Find the largest selection from all brands at the lowest prices in India.
    Payment options - COD, EMI, Credit card, Debit card &amp;amp; more.">

    <meta name="Keywords" content="accounting web , Invoice ,Accounting On Web,Sale Invoice,Purchase Bill,Inventory Management,
          Cash Book,Online Ledger ,Bussiness Online ,accountancy web ,accounting solutions">
    <meta name="google-site-verification" content="SjdanId64q7Oswietj1GXGEx2y5fLhMrFAleh7rIw_I" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!-- Font A  <!-- Font Awesome Icons -->
    <link href="~/fontawesome-free/css/all.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-XDKJ7NZW4H"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-XDKJ7NZW4H');
    </script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"> <i class="fab fa-soundcloud fa-1x"></i>Accounting On Web</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                          <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/AboutUs">About Us</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Pricing">Pricing</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/ContactUs">Contact Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>
                        
                        
                       
                        @if (Context.User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-page="Companies/Index">
                                    My Companies
                                </a>
                            </li>
                        }
                    </ul>
                    <div class="d-flex">

                        @if (Context.User.Identity.IsAuthenticated)
                        {
                            <span class="navbar-text">
                                Welcome @User.Identity.Name
                            </span>
                            <ul class="navbar-nav ms-auto flex-nowrap flex-grow-1 text-right">
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-controller="UserAccount" asp-action="Logout">Sign Out</a>
                                </li>
                            </ul>
                        }
                        else
                        {
                            <ul class="navbar-nav ms-auto flex-nowrap flex-grow-1 text-right">
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-page="/Account/Login">Login</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-page="/Account/Register">Register</a>
                                </li>

                            </ul>
                        }
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - WebApp.UI2 - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/axios/axios.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
  
    <!-- Messenger Chat plugin Code -->
    <div id="fb-root"></div>

    <!-- Your Chat plugin code -->
    <div id="fb-customer-chat" class="fb-customerchat">
    </div>

