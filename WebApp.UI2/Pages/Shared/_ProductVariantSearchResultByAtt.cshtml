@model IEnumerable<WebApp.UI2.Pages.MyBooks.ProductCategories.SearchResultByAttributeModel.ProductVariantsViewModel>

<table class="table">
    <thead>
        <tr>
            <th>
                <label>Name</label>
            </th>

            <th>
                <label>Size</label>
            </th>
           

            <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", "ProductVariants", new { id = item.Id }) |
                    @*@Html.ActionLink("Details", "Details", new { id = item.Id }) |*@
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>

</table>